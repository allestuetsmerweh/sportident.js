{
  "name": "sportident-root",
  "version": "1.0.0",
  "description": "JavaScript interface to SportIdent devices",
  "main": "src/build/si.node.js",
  "scripts": {
    "build": "lerna run build",
    "build-docs": "sh build-docs.sh",
    "dependency-check": "dependency-cruise --config=./.dependency-cruiser.js packages/*/src",
    "lerna": "lerna",
    "lint": "lerna run eslint",
    "publish": "lerna run tsc && lerna publish",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "testbench-client": "lerna run testbench-client",
    "testbench-node": "cd packages/sportident-testbench-node; npm run testbench-node",
    "testbench-server": "lerna run testbench-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allestuetsmerweh/sportident.js.git"
  },
  "keywords": [
    "sportident",
    "javascript",
    "js"
  ],
  "author": "Simon Hatt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allestuetsmerweh/sportident.js/issues"
  },
  "homepage": "https://github.com/allestuetsmerweh/sportident.js#readme",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@babel/preset-typescript": "^7.6.0",
    "@types/jest": "^24.0.21",
    "@types/lodash": "^4.14.144",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "dependency-cruiser": "^5.2.2",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "gh-pages": "^2.1.1",
    "html-loader": "^0.5.5",
    "jasmine-core": "^3.5.0",
    "jest": "^24.9.0",
    "lerna": "^3.18.3",
    "node": "^12.13.0",
    "source-map-loader": "^0.2.4",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
