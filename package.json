{
  "name": "sportident",
  "version": "1.0.0",
  "description": "JavaScript interface to SportIdent devices",
  "main": "si.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "webpack-src": "cd src/ && webpack",
    "webpack-testbench": "cd testbench/ && webpack",
    "build-docs": "sh build-docs.sh",
    "eslint": "eslint .",
    "jest": "jest",
    "dependency-check": "dependency-cruise --config=./.dependency-cruiser.js src testbench",
    "start-testbench": "cd testbench/ && webpack-dev-server",
    "testbench": "sh run-testbench.sh",
    "tests": "npm run eslint && npm run jest"
  },
  "jest": {
    "testRegex": ".*/.*\\.test\\.js",
    "collectCoverage": true,
    "maxConcurrency": 2
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/allestuetsmerweh/sportident.js.git"
  },
  "keywords": [
    "sportident",
    "javascript",
    "js"
  ],
  "author": "Simon Hatt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allestuetsmerweh/sportident.js/issues"
  },
  "homepage": "https://github.com/allestuetsmerweh/sportident.js#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "dependency-cruiser": "^4.21.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "gh-pages": "^2.0.1",
    "html-loader": "^0.5.5",
    "immutable": "^4.0.0-rc.12",
    "jasmine-core": "^3.4.0",
    "jest": "^24.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "static-site-generator-webpack-plugin": "^3.4.2",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-server": "^3.7.1"
  },
  "peerDependencies": {
    "@babel/polyfill": "^7.4.3",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
